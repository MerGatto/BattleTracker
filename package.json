{
  "name": "battle-tracker",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "prebuild": "git describe --tags --always > src/assets/version.txt",
    "build": "ng build",
    "start": "npm run prebuild && ng serve",
    "prod": "ng serve --configuration=production",
    "test": "ng test",
    "deploy": "ng deploy --base-href='/BattleTracker/'",
    "lint": "ng lint",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json --clean --format json"
  },
  "private": true,
  "dependencies": {
    "@angular-slider/ngx-slider": "^19.0.0",
    "@angular/animations": "^19.2.11",
    "@angular/cdk": "^19.2.16",
    "@angular/common": "19.2.11",
    "@angular/compiler": "19.2.11",
    "@angular/core": "19.2.11",
    "@angular/forms": "19.2.11",
    "@angular/platform-browser": "19.2.11",
    "@angular/platform-browser-dynamic": "19.2.11",
    "@angular/platform-server": "19.2.11",
    "@angular/router": "19.2.11",
    "@auth0/angular-jwt": "5.2.0",
    "@fortawesome/fontawesome-common-types": "6.7.2",
    "@fortawesome/fontawesome-free": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@ng-bootstrap/ng-bootstrap": "18.0.0",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@types/node": "22.15.19",
    "bootstrap": "5.3.6",
    "bootstrap-slider": "11.0.2",
    "core-js": "3.42.0",
    "jquery": "3.7.1",
    "rxjs": "7.8.2",
    "sass": "1.89.0",
    "ts-helpers": "1.1.2",
    "tslib": "2.8.1",
    "typescript": "5.8.3",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular/cli": "19.2.12",
    "@angular-devkit/build-angular": "20.0.1",
    "@angular/compiler-cli": "19.2.11",
    "@types/jasmine": "^5.1.8",
    "@vendure/ngx-translate-extract": "^9.4.0",
    "@web/test-runner": "^0.20.2",
    "angular-cli-ghpages": "^2.0.3",
    "angular-eslint": "19.4.0",
    "eslint": "^9.26.0",
    "jasmine-core": "^5.7.1",
    "jasmine-spec-reporter": "7.0.0",
    "ts-node": "10.9.2",
    "typescript-eslint": "8.32.0"
  }
}
